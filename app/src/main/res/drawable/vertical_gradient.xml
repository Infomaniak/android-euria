<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="100dp"
    android:height="100dp"
    android:viewportWidth="100"
    android:viewportHeight="100">

    <group>
        <clip-path android:pathData="M0,0 L100,0 L100,100 L0,100 Z" />
        <path android:pathData="M0,0 L100,0 L100,100 L0,100 Z">
            <aapt:attr xmlns:aapt="http://schemas.android.com/aapt"
                name="android:fillColor">
                <gradient
                    android:endX="0"
                    android:endY="0"
                    android:startX="0"
                    android:startY="100"
                    android:type="linear">
                    <item
                        android:color="#121318"
                        android:offset="0" />
                    <item
                        android:color="#FF24405b"
                        android:offset="1" />
                </gradient>
            </aapt:attr>
        </path>
    </group>
</vector>
